{{!< default}}
{{! The above will include this template into the body of the default template }}

{{> page-header}}
{{! This includes the page-header.hbs partial }}

<main id="main" class="container">
	<div id="wrapper" class="clearfix">

		<div id="content">
			<article class="post">
				<div class="row">
					<div class="col-md-12">
						<h1 class="page-title">Content not found</h1>
					</div>
				</div>

				<div class="row">
					<div class="col-md-12">
						<div class="post-content">
							<p>The content or page you are looking for was not found. Maybe you have the wrong address or it was moved.</p>
							<p>Navigate to our <a href="{{@blog.url}}">home page</a> for a list of our most recent articles or use the navigation to discover content.</p>
						</div>
					</div>
				</div>
			</article>
			<!-- / post -->
		</div>
		<!-- / content -->

		{{> page-sidebar}}
		{{! This includes the page-sidebar.hbs partial }}
	</div>
	<!-- / wrapper -->
</main>
<!-- / main -->

{{> page-footer}}
{{! This includes the page-footer.hbs partial }}